{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "node_modlues",
    "client",
    "tests"
  ]
}